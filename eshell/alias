alias ps proced
alias clear clear 1
alias open find-file $1
alias ls ls -la $1

alias dev cd ~/Developer
alias desk cd ~/Desktop
alias down cd ~/Downloads

alias init find-file ~/.emacs.d/init.el
alias alias find-file ~/.emacs.d/eshell/alias

alias psql sql-postgres
alias cdr cd (projectile-project-root)
alias grep *grep

alias python python3 $*
alias pip pip3 $*
alias pshell pipenv shell
alias pytest pytest -svv
